config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11' #1, 6, or 11
	option hwmode '11g'
	option path 'soc/soc:pcie/pci0000:00/0000:00:02.0/0000:02:00.0' #shelby and caiman
	#option path 'soc/soc:pcie@82000000/pci0000:00/0000:00:02.0/0000:02:00.0' #mamba
	option htmode 'HT20'
	option country 'US'
	option txpower '30'
	option vendor_vht '1'
	option cell_density '0'

config wifi-iface '24Example3'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Example-WPA3'
	option encryption 'sae'
	option sae_pwe '0'
	option key 'password for wpa3 devices'
	option ieee80211w '2'
	option wpa_disable_eapol_key_retries '1'
	option multicast_to_unicast '0'
	option isolate '0'

config wifi-iface '24Example'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Example'
	option encryption 'psk2+ccmp'
	option key 'password for wpa2 devices'
	option wpa_disable_eapol_key_retries '1'
	option multicast_to_unicast '0'
	option isolate '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '149' #36 or 149, DFS is broken on these devices
	option hwmode '11a'
	option path 'soc/soc:pcie/pci0000:00/0000:00:01.0/0000:01:00.0' #shelby and caiman
	#option path 'soc/soc:pcie@82000000/pci0000:00/0000:00:03.0/0000:03:00.0' #mamba
	option htmode 'VHT80'
	option country 'US'
	option txpower '30'
	option cell_density '0'

config wifi-iface '5Example3'
